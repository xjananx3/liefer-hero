@model IEnumerable<Speise>

@{
    
}

<h1>Unsere Speisen</h1>

<table class="table table-bordered">
    <thead>
    <tr>
        <th>Nummer</th>
        <th>Name</th>
        <th>Preis</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Preis</td>
            <td></td>
        </tr>
    }
    </tbody>
</table>